<% provide(:title, "Project") %>
<h1>Project Manager</h1>
<div class="row">
  <div class="span6 offset3">
  	<%= form_for(@project) do |f| %>
      <%= render 'shared/error_projects', object: f.object %>
  		<%= f.label :name %>
  		<%= f.text_field :name %>
  		
  		<%= f.label :description %>
  		<%= f.text_field :description %>
  		
  	
  		<%= f.submit "Save", class: "btn btn-large btn-primary" %>
  	<% end %>
  </div>
</div>
